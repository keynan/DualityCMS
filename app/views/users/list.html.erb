<%= render :partial => 'admin/sub_nav' %>
<%= link_to 'New User', :action => 'new' %>
<ul>
	<% @users.each do  |user| %>
		<li id="user_<%=h user.id %>">
			<%=h user.full_name %>
			<%= link_to 'Edit', :action => 'edit', :id => user.id %>
			<%= link_to 'Delete', user_path(user.id), {:method => :delete}  %>
		</li>
	<% end %>
</ul>